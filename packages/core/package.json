{
  "name": "@gym/core",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./services/*": {
      "types": "./dist/services/*.d.ts",
      "default": "./dist/services/*.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:unit": "vitest run"
  },
  "dependencies": {
    "@gym/database": "workspace:*",
    "@gym/shared": "workspace:*",
    "bcryptjs": "^2.4.3",
    "stripe": "^20.1.1",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "vitest": "^1.1.0"
  }
}
